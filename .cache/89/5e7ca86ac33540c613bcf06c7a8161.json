"column":6},"source":"src/js/views/RecipeView.js","original":{"line":142,"column":4}},{"generated":{"line":87,"column":21},"source":"src/js/views/RecipeView.js","original":{"line":142,"column":19}},{"name":"innerHTML","generated":{"line":87,"column":22},"source":"src/js/views/RecipeView.js","original":{"line":142,"column":20}},{"generated":{"line":87,"column":31},"source":"src/js/views/RecipeView.js","original":{"line":142,"column":4}},{"name":"spinnerHTML","generated":{"line":87,"column":34},"source":"src/js/views/RecipeView.js","original":{"line":142,"column":32}},{"generated":{"line":87,"column":45},"source":"src/js/views/RecipeView.js","original":{"line":142,"column":4}},{"generated":{"line":88,"column":0},"source":"src/js/views/RecipeView.js","original":{"line":143,"column":3}},{"generated":{"line":94,"column":15},"source":"src/js/views/RecipeView.js","original":{"line":146,"column":15}},{"name":"RecipeView","generated":{"line":94,"column":19},"source":"src/js/views/RecipeView.js","original":{"line":146,"column":19}},{"generated":{"line":94,"column":29},"source":"src/js/views/RecipeView.js","original":{"line":146,"column":15}}],"sources":{"src/js/views/RecipeView.js":"import Fractional from 'fractional';\r\nimport icons from '../../img/icons.svg';\r\nconst recipeContainer = document.querySelector('.recipe');\r\n\r\nclass RecipeView {\r\n  async _renderRecipe(recipe) {\r\n    this._renderSpinner();\r\n    if (!recipe) return;\r\n\r\n    const recipeHTML = `\r\n          <figure class=\"recipe__fig\">\r\n            <img src=\"${recipe.imageURL}\" alt=\"\" class=\"recipe__img\" />\r\n            <h1 class=\"recipe__title\">\r\n              <span>${recipe.title}</span>\r\n            </h1>\r\n          </figure>\r\n  \r\n          <div class=\"recipe__details\">\r\n            <div class=\"recipe__info\">\r\n              <svg class=\"recipe__info-icon\">\r\n                <use href=\"${icons}#icon-clock\"></use>\r\n              </svg>\r\n              <span class=\"recipe__info-data recipe__info-data--minutes\">${\r\n                recipe.cookingTime\r\n              }</span>\r\n              <span class=\"recipe__info-text\">minutes</span>\r\n            </div>\r\n            <div class=\"recipe__info\">\r\n              <svg class=\"recipe__info-icon\">\r\n                <use href=\"${icons}#icon-users\"></use>\r\n              </svg>\r\n              <span class=\"recipe__info-data recipe__info-data--people\">${\r\n                recipe.servings\r\n              }</span>\r\n              <span class=\"recipe__info-text\">servings</span>\r\n  \r\n              <div class=\"recipe__info-buttons\">\r\n                <button class=\"btn--tiny btn--increase-servings\">\r\n                  <svg>\r\n                    <use href=\"${icons}#icon-minus-circle\"></use>\r\n                  </svg>\r\n                </button>\r\n                <button class=\"btn--tiny btn--increase-servings\">\r\n                  <svg>\r\n                    <use href=\"${icons}#icon-plus-circle\"></use>\r\n                  </svg>\r\n                </button>\r\n              </div>\r\n            </div>\r\n  \r\n            <div class=\"recipe__user-generated\">\r\n              <svg>\r\n                <use href=\"${icons}#icon-user\"></use>\r\n              </svg>\r\n            </div>\r\n            <button class=\"btn--round\">\r\n              <svg class=\"\">\r\n                <use href=\"${icons}#icon-bookmark-fill\"></use>\r\n              </svg>\r\n            </button>\r\n          </div>\r\n  \r\n          <div class=\"recipe__ingredients\">\r\n            <h2 class=\"heading--2\">Recipe ingredients</h2>\r\n            <ul class=\"recipe__ingredient-list\">   \r\n        ${recipe.ingredients\r\n          .map(\r\n            ingredient => `\r\n            <li class=\"recipe__ingredient\">\r\n              <svg class=\"recipe__icon\">\r\n              <use href=\"${icons}#icon-check\"use>\r\n               </svg>\r\n              <div class=\"recipe__quantity\">${new Fractional.Fraction(\r\n                ingredient.quantity\r\n              ).toString()}</div>\r\n               <div class=\"recipe__description\">\r\n              <span class=\"recipe__unit\">${ingredient.unit}</span>\r\n                ${ingredient.description}\r\n            </div>\r\n            </li>\r\n            `\r\n          )\r\n          .join('')}     \r\n            </ul>\r\n          </div>\r\n  \r\n          <div class=\"recipe__directions\">\r\n            <h2 class=\"heading--2\">How to cook it</h2>\r\n            <p class=\"recipe__directions-text\">\r\n              This recipe was carefully designed and tested by\r\n              <span class=\"recipe__publisher\">The Pioneer Woman</span>. Please check out\r\n              directions at their website.\r\n            </p>\r\n            <a\r\n              class=\"btn--small recipe__btn\"\r\n              href=\"${recipe.sourceURL}\"\r\n              target=\"_blank\"\r\n            >\r\n              <span>Directions</span>\r\n              <svg class=\"search__icon\">\r\n                <use href=\"${icons}#icon-arrow-right\"></use>\r\n              </svg>\r\n            </a>\r\n          </div>\r\n          \r\n    `;\r\n\r\n    recipeContainer.innerHTML = recipeHTML;\r\n  }\r\n\r\n  _renderError() {\r\n    const message = 'No recipes found for your query. Please try again!';\r\n\r\n    const errMessageHTML = `\r\n      <div class=\"error\">\r\n        <div>\r\n          <svg>\r\n            <use href=\"${icons}#icon-alert-triangle\"></use>\r\n          </svg>\r\n        </div>\r\n        <p>${message}</p>\r\n      </div>\r\n    `;\r\n\r\n    recipeContainer.innerHTML = errMessageHTML;\r\n  }\r\n\r\n  _addHandlerRender(handler) {\r\n    ['hashchange', 'load'].forEach(event =>\r\n      window.addEventListener(event, handler)\r\n    );\r\n  }\r\n\r\n  _renderSpinner() {\r\n    const spinnerHTML = `\r\n        <div class=\"spinner\">\r\n            <svg>\r\n              <use href=\"${icons}#icon-loader\"></use>\r\n            </svg>\r\n        </div>\r\n    `;\r\n    recipeContainer.innerHTML = spinnerHTML;\r\n  }\r\n}\r\n\r\nexport default new RecipeView();\r\n"},"lineCount":null}},"error":null,"hash":"61faab2674b490980c670cca8f3c4457","cacheData":{"env":{}}}